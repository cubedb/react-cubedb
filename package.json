{
  "name": "react-cubedb",
  "version": "0.2.0",
  "description": "Ract client to CubeDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/jrfferreira/react-cubedb.git"
  },
  "author": "sztanko, jrfferreira",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/jrfferreira/react-cubedb/issues"
  },
  "homepage": "https://github.com/jrfferreira/react-cubedb",
  "keywords": [
    "react-component",
    "react",
    "d3"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "doc": "react-doc-generator src -o DOCUMENTATION.md"

  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.5.4",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "d3": "^4.9.1",
    "lodash": "^4.17.4",
    "react-bootstrap": "^0.31.0",
    "react-doc-generator": "^1.2.5"
  }
}
